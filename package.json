{
  "name": "angelblanco-dev-blog",
  "description": "My blog, built on sapper",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "blog": "node ./blog",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --entry '/ /404'",
    "export:production": "yarn export --host='angelblanco.dev' dist",
    "sirv:production": "sirv dist",
    "sirv": "sirv __sapper__/export ",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "scss:lint": "stylelint 'src/**/*.scss'",
    "scss:fix": "stylelint 'src/**/*.scss' --fix",
    "docker:build": "docker build -t angelblanco.dev .",
    "docker:sirv": "docker run -i -t -p 8081:8081 angelblanco.dev:latest && echo 'Running on http://localhost:8081//'"
  },
  "dependencies": {
    "commander": "^6.1.0",
    "compression": "^1.7.1",
    "inquirer": "^7.3.2",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "polka": "^1.0.0-next.9",
    "reading-time": "^1.2.0",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-replace": "^2.0.0",
    "bulma": "^0.9.0",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "escape-html": "^1.0.3",
    "flexsearch": "^0.6.32",
    "front-matter": "^4.0.2",
    "globby": "^11.0.1",
    "jsdom": "^16.2.2",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "prismjs": "^1.17.1",
    "rollup": "^2.27.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-pluginutils": "^2.8.2",
    "sapper": "^0.28.7",
    "sirv-cli": "^1.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.12.1",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.2.0"
  }
}
